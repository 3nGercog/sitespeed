@{
    ViewBag.Title = "Site Speed";
}



<div id="box" class="jxgbox" style="width:100%; height:500px; margin-bottom: 20px;"></div>

<div id="urltable">
    @Html.Partial("_TableView")
</div>

<ul  class="pager">
    @{
        List<sitespeed.Models.History> hst = (List<sitespeed.Models.History>)ViewData["graf"];
        int count = 1;
    }
    @for (int i = 0; i < hst.Count; i = i + 20)
    {
        <li>@Ajax.ActionLink(count++.ToString(), "Next",new { query = "next", startIndex = i, pageSize = 20 }, new AjaxOptions { HttpMethod = "POST", InsertionMode = InsertionMode.Replace, UpdateTargetId = "urltable" })</li>
    }
    
</ul>

